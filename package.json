{
    "name": "bit-client",
    "version": "1.0.0",
    "layaair": {
        "version": "3.1.0"
    },
    "scripts": {
        "build-proto:pbjs": "pbjs --dependency protobufjs/minimal.js --target static-module --wrap commonjs --out ./assets/script/def/proto.js ./assets/proto/*.proto",
        "build-proto:pbts": "pbts --main --out ./assets/script/def/proto.d.ts ./assets/script/def/*.js"
    },
    "dependencies": {
        "bytebuffer": "^5.0.1",
        "protobufjs": "^7.2.5"
    },
    "devDependencies": {
        "@types/bytebuffer": "^5.0.48"
    }
}
